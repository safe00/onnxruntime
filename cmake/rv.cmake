SET(CMAKE_SYSTEM_NAME Linux)
SET(CMAKE_SYSTEM_VERSION 1)
SET(CMAKE_C_COMPILER riscv64-unknown-linux-gnu-gcc)
SET(CMAKE_CXX_COMPILER riscv64-unknown-linux-gnu-g++)
SET(CMAKE_FIND_ROOT_PATH_MODE_PROGRAM NEVER)
SET(CMAKE_FIND_ROOT_PATH_MODE_LIBRARY ONLY)
SET(CMAKE_FIND_ROOT_PATH_MODE_INCLUDE ONLY)
SET(CMAKE_FIND_ROOT_PATH_MODE_PACKAGE ONLY)
SET(onnxruntime_USE_RISCV ON)
SET(onnxruntime_BUILD_SHARED_LIB ON)
SET(onnxruntime_BUILD_UNIT_TESTS ON)
SET(onnxruntime_ENABLE_CPUINFO OFF)
SET(onnxruntime_CROSS_COMPILING ON)
SET(onnxruntime_DISABLE_RTTI OFF)
SET(onnxruntime_USE_SHL ON)
SET(onnxruntime_SHL_TARGET C920)
SET(onnxruntime_SHL_HOME /shared/onnxruntime/csi-nn2/install_nn2/)
